{
  "name": "cognihub",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch ./ --watch server.ts --exec \"node --loader ts-node/esm --experimental-specifier-resolution=node server.ts\"",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm install",
    "type-check": "npx tsc --noEmit",
    "build": "tsc && cp package.json ../dist/"
  },
  "nodemonConfig": {
    "watch": [
      "./**/*",
      "server.ts"
    ],
    "ext": "ts, js, html",
    "exec": "node --loader ts-node/esm --experimental-specifier-resolution=node server.ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}